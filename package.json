{
  "name": "whats-the-score-instant",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -b && vite build",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "lint:code": "eslint src --max-warnings 0",
    "lint": "run-p format:check lint:code",
    "preview": "vite preview",
    "test": "TZ=UTC vitest",
    "test:ci": "CI=1 TZ=UTC vitest run --coverage"
  },
  "dependencies": {
    "@instantdb/react": "^0.12.6",
    "@tanstack/react-router": "^1.50.1",
    "classnames": "^2.5.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "remeda": "^2.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@tanstack/router-plugin": "^1.49.3",
    "@types/node": "^22.5.0",
    "@types/react": "^18.3.6",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.0.5",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "vitest": "^2.0.5"
  }
}
